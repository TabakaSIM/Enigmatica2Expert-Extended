{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "files": [
    "./**/*js"
  ],
  "scripts": {
    "ðŸ§±build": "bun dev/make_pack.ts",
    "bansoukou-prune": "for f in mods/*-patched.jar; do mv -v '${f%-patched.jar}.disabled' '${f%-patched.jar}.jar' && rm -v '$f'; done",
    "âš¡dobious-perf": "sed -i 's/listAllPartMaterials=true/listAllPartMaterials=false/' config/tconstruct.cfg && git update-index --assume-unchanged config/tconstruct.cfg && sed -i 's/NBTTagImprovements=true/NBTTagImprovements=false/' config/stellar_core.cfg && git update-index --assume-unchanged config/stellar_core.cfg",
    "dev": "concurrently --prefix-colors auto \"npm:dev:* --silent\"",
    "dev:bansoukou": "bun dev/automation/bansoukou.js",
    "dev:benchmark": "bunx mc-benchmark > dev/benchmarks/latest.md",
    "dev:conflicts": "grep 'conflicts with' crafttweaker.log | sort | uniq | grep -Evf dev/tools/conflict.ignore | sed \"s/^[^ ]* //\" >&2",
    "dev:entities": "bun dev/automation/entities.js",
    "dev:errors": "bun mc-tools/packages/errors/src/cli.ts --config=dev/tools/mct-errors-config.yml --output=dev/tools/unresolved-errors.log",
    "dev:ftbq_cleanup": "bun mc-tools/scripts/ftbq_cleanup.ts",
    "dev:inject_js": "bun dev/automation/Inject_JS.js",
    "dev:jei": "bun dev/automation/jei.js",
    "dev:jer": "bun dev/automation/jer.ts",
    "dev:manifest": "bun mc-tools/packages/manifest/src/cli.ts --verbose --ignore=dev/.devonly.ignore --key=~secrets/cf_api_key.txt",
    "dev:misc": "bun dev/automation/misc.js --keep-cache",
    "dev:modlist": "bun mc-tools/packages/modlist/src/cli.ts --old=../E2E-unchanged/minecraftinstance.json --verbose --ignore=dev/.devonly.ignore --key=~secrets/cf_api_key.txt --template=dev/tools/modlist_template.md --sort=addonID",
    "dev:tcon": "bun mc-tools/packages/tcon/src/cli.ts --default=dev/default_configs/tweakersconstruct.cfg --tweaks=dev/tools/tcon/tweaks --save=dev/tools/tcon/stats",
    "dev:tips": "bun dev/automation/tips.js",
    "dev:xml": "bun dev/automation/XML.js",
    "tellme-prune": "bash -c \"for prefix in \\$(ls -1 config/tellme/*.csv | xargs -n 1 basename | sed -E 's/(_[0-9].*)?\\\\.csv//' | sort -u); do ls -1 config/tellme/\\\"\\${prefix}\\\"*.csv | sort -r | tail -n +2 | xargs -r rm; done\"",
    "reducer-levels": "bun mc-tools/packages/reducer/src/cli.ts levels dev/tools/reduce_mods.json",
    "reducer-interactive": "bun mc-tools/packages/reducer/src/cli.ts interactive"
  },
  "dependencies": {
    "cheerio": "^1.1.0",
    "markdown-it": "^14.1.0",
    "mdimg": "^1.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.16.1",
    "@clack/prompts": "^0.11.0",
    "@conventional-changelog/git-client": "^2.5.1",
    "@types/adm-zip": "^0.5.7",
    "@types/fast-levenshtein": "^0.0.4",
    "@types/fs-extra": "^11.0.4",
    "@types/memoizee": "^0.4.12",
    "@types/ssh2-sftp-client": "^9.0.4",
    "@types/terminal-kit": "^2.5.7",
    "adm-zip": "^0.5.16",
    "boxen": "^8.0.1",
    "chalk": "^5.4.1",
    "cli-progress": "^3.12.0",
    "compare-func": "^2.0.0",
    "concurrently": "^9.2.0",
    "consola": "^3.4.2",
    "conventional-changelog": "^7.1.1",
    "conventional-changelog-writer": "^8.2.0",
    "conventional-commits-parser": "^6.2.0",
    "csv-parse": "^5.6.0",
    "curseforge-v2": "^1.4.0",
    "del": "^8.0.0",
    "detect-indent": "^7.0.1",
    "eslint": "^9.29.0",
    "fast-glob": "^3.3.3",
    "fast-levenshtein": "^3.0.0",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.3",
    "humanize-string": "^3.0.0",
    "ignore": "^7.0.5",
    "language-name-map": "^0.3.0",
    "lodash": "^4.17.21",
    "log-update": "^6.1.0",
    "mc-benchmark": "^2.1.0",
    "memoizee": "^0.4.17",
    "nbt-ts": "^1.3.6",
    "node-fetch": "^3.3.2",
    "node-pty": "^1.0.0",
    "parse-gitignore": "2.0.0",
    "pathe": "^2.0.3",
    "pdf-parse": "^1.1.1",
    "prismarine-nbt": "^2.7.0",
    "replace-in-file": "^8.3.0",
    "rimraf": "^6.0.1",
    "simplify-js": "^1.2.4",
    "snake-case": "^4.0.0",
    "ssh2-sftp-client": "^12.0.1",
    "table": "^6.9.0",
    "terminal-kit": "^3.1.2",
    "tinyglobby": "^0.2.14",
    "typescript": "^5.8.3",
    "xml-js": "^1.6.11",
    "yaml": "^2.8.0",
    "yargs": "^18.0.0",
    "zx": "^8.7.2"
  }
}
